<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM "hibernate-reverse-engineering-3.0.dtd">

<hibernate-reverse-engineering>
    <schema-selection match-schema="public" />
    <type-mapping>
        <!-- order is very important specific cases first everything without decimal
            digits should be mapped to long -->

        <sql-type jdbc-type="NUMERIC" hibernate-type="int" not-null="true"
                  scale="0" precision="3">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="int" not-null="true"
                  scale="0" precision="4">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="Integer"
                  not-null="false" scale="0" precision="4">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="Integer"
                  not-null="false" scale="0" precision="3">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="int" not-null="true"
                  scale="0" precision="8">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="Integer"
                  not-null="false" scale="0" precision="8">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="long" not-null="true"
                  scale="0">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="Long" scale="0"
                  not-null="false">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="double"
                  not-null="true">
        </sql-type>
        <sql-type jdbc-type="NUMERIC" hibernate-type="Double"
                  not-null="false">
        </sql-type>
        <sql-type jdbc-type="DATE" hibernate-type="java.time.LocalDate">
        </sql-type>
        <sql-type jdbc-type="TIMESTAMP" hibernate-type="java.time.Instant">
        </sql-type>
    </type-mapping>
    <table-filter match-name="mf.*" match-schema="public" package="de.hf.dac.marketdataprovider.api.domain"/>
    <!--<table-filter match-name="md_product" match-schema="public" package="de.hf.dac.marketdataprovider.api.domain"/>-->


    <table schema="public" name="mf_instrument" class="de.hf.dac.marketdataprovider.api.domain.Instrument">
        <primary-key>
            <generator class="identity"></generator>
        </primary-key>
    </table>

    <table schema="public" name="mf_securitysymbols" class="de.hf.dac.marketdataprovider.api.domain.SecuritySymbols">
        <primary-key>
            <generator class="identity">
            </generator>
        </primary-key>
    </table>

    <table schema="public" name="mf_source" class="de.hf.dac.marketdataprovider.api.domain.Source">
        <primary-key>
            <generator class="identity">
            </generator>
        </primary-key>
    </table>

    <table schema="public" name="mf_currency" class="de.hf.dac.marketdataprovider.api.domain.Currency"/>
    <table schema="public" name="mf_endofdayprice" class="de.hf.dac.marketdataprovider.api.domain.EndOfDayPrice"/>
    <table schema="public" name="mf_importtype" class="de.hf.dac.marketdataprovider.api.domain.ImportType"/>
    <table schema="public" name="mf_instrumenttype" class="de.hf.dac.marketdataprovider.api.domain.InstrumentType"/>
    <table schema="public" name="mf_security" class="de.hf.dac.marketdataprovider.api.domain.Security"/>
    <table schema="public" name="mf_securitytyp" class="de.hf.dac.marketdataprovider.api.domain.SecurityType"/>


</hibernate-reverse-engineering>